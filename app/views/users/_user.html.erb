<div id="<%= dom_id user %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">First name:</strong>
    <%= user.first_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Last name:</strong>
    <%= user.last_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Username:</strong>
    <%= user.username %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= user.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Date of birth:</strong>
    <%= user.date_of_birth %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Role:</strong>
    <%= user.role %>
  </div>
</div>
